    "@react-native-firebase/auth": "^6.7.1",
    "@react-navigation/bottom-tabs": "^5.5.1",
    "@react-navigation/native": "^5.5.0",
    "@react-navigation/stack": "^5.4.1",
    "axios": "^0.19.2",
    "expo-facebook": "^9.0.0",
